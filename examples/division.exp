$nln = 10

$cycle - 
times {
	read -> $a
	read -> $b
	
	$res = 0
	$a times {	
		$r = $a
		$res times { $b times { $r- } }
		
		$ok = 0
		$less_zero = $r < 0 times { $ok+ times { $res- } }
		$is_zero = $r ? times { $ok+ }
		
		$ok > 0 times { break }
		ELSE times { $res+ }
	}
	$res print

	$nln printc
}